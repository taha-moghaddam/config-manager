{
    "name": "laravel-admin-ext/config",
    "description": "Config extension for bCore",
    "type": "library",
    "keywords": ["bcore", "setting"],
    "homepage": "https://gitlab.dr8.ir/bikaraan/laravel-admin-ext-config-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "z-song",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.5",
        "bikaraan/bcore": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "Bikaraan\\BCore\\Config\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bikaraan\\BCore\\Config\\ConfigServiceProvider"
            ]
        }
    }
}
